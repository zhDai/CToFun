 * @author: Shaola
 */

public class FrameInfo {
	private String ncfilename;
	private int time;
	
	FrameInfo(String nc, int t) {
		ncfilename = nc;
		time = t;
	}

	public void setNcfilename(String ncfilename) {
		this.ncfilename = ncfilename;
	}

	public void setTime(int time) {
		this.time = time;
	}
	
	public String getNcfilename() {
		return ncfilename;
	}
	
	public int getTime() {
		return time;
	}
}
