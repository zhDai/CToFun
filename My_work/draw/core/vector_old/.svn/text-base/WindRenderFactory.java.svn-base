package cn.ct.em.draw.core.vector_old;

public class WindRenderFactory {
	public static WindRenderCore getMercatorSVGArrow(){
		WindRenderCore obj = new WindRenderCore();
	    obj.windbridge = new WindRenderSVGArrow();
	    obj.windbridge.windRenderCore = obj;
	    return obj;
	}
	
	public static WindRenderCore getMercatorSVGBarb(){
		WindRenderCore obj = new WindRenderCore();
	    obj.windbridge = new WindRenderSVGBarb();
	    obj.windbridge.windRenderCore = obj;
	    return obj;
	}

}
